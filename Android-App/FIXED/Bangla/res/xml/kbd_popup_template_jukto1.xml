<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="10%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="@dimen/key_height"
    >

    <Row>
        <Key android:codes="801,802" android:keyIcon="@drawable/kto1" android:keyEdgeFlags="left" android:popupKeyboard="@xml/kbd_popup_template_q1"/>
        <Key android:codes="803,804,805" android:keyIcon="@drawable/shwo" android:popupKeyboard="@xml/kbd_popup_template_w1"/>
        <Key android:codes="806,807,808" android:keyIcon="@drawable/shko1" android:popupKeyboard="@xml/kbd_popup_template_e1"/>
        <Key android:codes="809,810,811" android:keyIcon="@drawable/shto1" android:popupKeyboard="@xml/kbd_popup_template_r1" />
        <Key android:codes="812,823,814" android:keyIcon="@drawable/tmo" android:popupKeyboard="@xml/kbd_popup_template_t1"/>
        <Key android:codes="816,817" android:keyIcon="@drawable/tto" android:popupKeyboard="@xml/kbd_popup_template_y1"/>
        <Key android:codes="818,819,820" android:keyIcon="@drawable/ndwo" android:popupKeyboard="@xml/kbd_popup_template_u1"/>
        <Key android:codes="821,822,823" android:keyIcon="@drawable/nto1" android:popupKeyboard="@xml/kbd_popup_template_i1"/>
        <Key android:codes="824,825" android:keyIcon="@drawable/ld1"  android:popupKeyboard="@xml/kbd_popup_template_o1"/>
        <Key android:codes="826,827,828" android:keyIcon="@drawable/pt" android:popupKeyboard="@xml/kbd_popup_template_p1" android:keyEdgeFlags="right"/>
    </Row>
    
    <Row>
        <Key android:codes="829,830,831" android:keyIcon="@drawable/sfo" android:horizontalGap="5%p" 
                android:keyEdgeFlags="left" android:popupKeyboard="@xml/kbd_popup_template_a1"/>
        <Key android:codes="832,833,834" android:keyIcon="@drawable/sko" android:popupKeyboard="@xml/kbd_popup_template_s1"/>
        <Key android:codes="835,836" android:keyIcon="@drawable/dbo" android:popupKeyboard="@xml/kbd_popup_template_d1"/>
        <Key android:codes="837" android:keyIcon="@drawable/flo"/>
        <Key android:codes="838,839,840" android:keyIcon="@drawable/ggo" android:popupKeyboard="@xml/kbd_popup_template_g1"/>
        <Key android:codes="841,842" android:keyIcon="@drawable/hno" android:popupKeyboard="@xml/kbd_popup_template_h1"/>
        <Key android:codes="843,844" android:keyIcon="@drawable/jjo" android:popupKeyboard="@xml/kbd_popup_template_j1"/>
        <Key android:codes="845,846,920" android:keyIcon="@drawable/kkho" android:popupKeyboard="@xml/kbd_popup_template_k1"/>
        <Key android:codes="847,848" android:keyIcon="@drawable/lko"  android:popupKeyboard="@xml/kbd_popup_template_l1" android:keyEdgeFlags="right"/>
    </Row>
    
    <Row>
        <Key android:codes="-1" android:keyIcon="@drawable/sym_keyboard_shift" 
                android:keyWidth="15%p" android:isModifier="true"
                android:isSticky="true" android:keyEdgeFlags="left"/>
        <Key android:codes="849,850" android:keyIcon="@drawable/dhwo" android:popupKeyboard="@xml/kbd_popup_template_z1"/>
        <Key android:codes="851,852" android:keyIcon="@drawable/ngo" android:popupKeyboard="@xml/kbd_popup_template_x1"/>
        <Key android:codes="853" android:keyIcon="@drawable/cco"/>
        <Key android:codes="854" android:keyIcon="@drawable/vlo" android:popupKeyboard="@xml/kbd_popup_template_v1"/>
        <Key android:codes="855,856" android:keyIcon="@drawable/bbo" android:popupKeyboard="@xml/kbd_popup_template_b1"/>
        <Key android:codes="857,858,859" android:keyIcon="@drawable/nno" android:popupKeyboard="@xml/kbd_popup_template_n1"/>
        <Key android:codes="860,861,862,863" android:keyIcon="@drawable/mmo" android:popupKeyboard="@xml/kbd_popup_template_m1"/>
        <Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete" 
                android:keyWidth="15%p" android:keyEdgeFlags="right"
                android:isRepeatable="true"/>
    </Row>
    
    <Row android:rowEdgeFlags="bottom">
        <Key android:codes="-3" android:keyIcon="@drawable/sym_keyboard_done" 
                android:keyWidth="20%p" android:keyEdgeFlags="left"/>
        <Key android:codes="-2" android:keyIcon="@drawable/ko" android:keyWidth="15%p"/>
        <Key android:codes="962" android:keyIcon="@drawable/sym_keyboard_space" 
                android:keyWidth="30%p" android:isRepeatable="true"/>
    <Key android:codes="961,960" android:keyIcon="@drawable/coma" android:popupKeyboard="@xml/kbd_popup_template_coma" android:keyWidth="15%p" />
        <Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return" 
                android:keyWidth="20%p" android:keyEdgeFlags="right"/>
    </Row>
</Keyboard>
    