<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="10%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="@dimen/key_height"
    >

    <Row>
        <Key android:codes="949" android:keyIcon="@drawable/z11" android:keyEdgeFlags="left"/>
        <Key android:codes="950" android:keyIcon="@drawable/z22"/>
        <Key android:codes="951" android:keyIcon="@drawable/z33"/>
        <Key android:codes="952" android:keyIcon="@drawable/z44"/>
        <Key android:codes="953" android:keyIcon="@drawable/z55"/>
        <Key android:codes="954" android:keyIcon="@drawable/z66"/>
        <Key android:codes="955" android:keyIcon="@drawable/z77"/>
        <Key android:codes="956" android:keyIcon="@drawable/z88"/>
        <Key android:codes="957" android:keyIcon="@drawable/z99"/>
        <Key android:codes="958" android:keyIcon="@drawable/z00" android:keyEdgeFlags="right"/>
    </Row>
    
    <Row>
        <Key android:codes="1049" android:keyIcon="@drawable/z1" android:keyEdgeFlags="left"/>
        <Key android:codes="1050" android:keyIcon="@drawable/z2"/>
        <Key android:codes="1051" android:keyIcon="@drawable/z3"/>
        <Key android:codes="1052" android:keyIcon="@drawable/z4"/>
        <Key android:codes="1053" android:keyIcon="@drawable/z5"/>
        <Key android:codes="1054" android:keyIcon="@drawable/z6"/>
        <Key android:codes="1055" android:keyIcon="@drawable/z7"/>
        <Key android:codes="1056" android:keyIcon="@drawable/z8"/>
        <Key android:codes="1057" android:keyIcon="@drawable/z9"/>
        <Key android:codes="1058" android:keyIcon="@drawable/z0" android:keyEdgeFlags="right"/>
    </Row>
    
    <Row>
        <Key android:codes="-1" android:keyIcon="@drawable/sym_keyboard_shift"
                android:keyWidth="15%p" android:isModifier="true"
                android:isSticky="true" android:keyEdgeFlags="left"/>
        <Key android:codes="963" android:keyLabel="!" />
        <Key android:codes="964" android:keyLabel="&quot;"/>
        <Key android:codes="965" android:keyLabel="\'"/>
        <Key android:codes="966" android:keyLabel=":"/>
        <Key android:codes="967" android:keyLabel=";"/>
        <Key android:codes="968" android:keyLabel="\@" />
        <Key android:codes="969" android:keyIcon="@drawable/que"/>
        <Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete"
                android:keyWidth="15%p" android:keyEdgeFlags="right"
                android:isRepeatable="true"/>
    </Row>
    
    <Row  android:rowEdgeFlags="bottom">
        <Key android:codes="-3" android:keyIcon="@drawable/sym_keyboard_done"
                android:keyWidth="20%p" android:keyEdgeFlags="left" />
        <Key android:codes="-2" android:keyIcon="@drawable/ko" android:keyWidth="15%p" />
        <Key android:codes="962" android:keyIcon="@drawable/sym_keyboard_space" android:keyWidth="30%p" 
                android:isRepeatable="true"/>
        <Key android:codes="961,960" android:keyIcon="@drawable/coma" android:popupKeyboard="@xml/kbd_popup_template_coma" android:keyWidth="15%p" />
        <Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return"
                android:keyWidth="20%p" android:keyEdgeFlags="right"
                />
    </Row>
</Keyboard>